!#define MyNFont  xft:Noto Mono:medium:antialias=true:size
!#define MyBFont  xft:Noto Mono:medium:antialias=true:size
!#define MyIFont  xft:Noto Mono:italic:antialias=true:size
!#define MyBIFont xft:Noto Mono:italic:antialias=true:size

